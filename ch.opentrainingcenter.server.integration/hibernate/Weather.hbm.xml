<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ch.opentrainingcenter.transfer.impl.Weather" table="WEATHER">
        <id name="id" type="int">
            <column name="ID" />
            <generator class="assigned" />
        </id>
        <property name="wetter" type="string">
            <column name="WETTER" />
        </property>
        <property name="imageicon" type="string">
            <column name="IMAGEICON" />
        </property>
    </class>
</hibernate-mapping>
