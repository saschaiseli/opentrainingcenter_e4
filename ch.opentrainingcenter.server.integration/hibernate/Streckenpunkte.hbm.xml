<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="ch.opentrainingcenter.transfer.impl.Streckenpunkte"
		table="STRECKENPUNKTE">
		<id name="id" type="int">
			<column name="ID" />
			<generator class="sequence">
				<param name="sequence">streckenpunkte_id_sequence</param>
			</generator>
		</id>
		<property name="longitude" type="java.lang.Double">
			<column name="LONGITUDE" precision="17" scale="0" />
		</property>
		<property name="latitude" type="java.lang.Double">
			<column name="LATITUDE" precision="17" scale="0" />
		</property>
	</class>
</hibernate-mapping>
